#!/usr/bin/python
def factorize_number(number):
    factors = []
    divisor = 2

    while number > 1:
        if number % divisor == 0:
            factors.append(divisor)
            number //= divisor
        else:
            divisor += 1

    return factors

def main():
    with open("tests/test00", "r") as file:
        for line in file:
            number = int(line.strip())
            factors = factorize_number(number)
            if factors:
                # Divide the list of factors into two parts
                half = len(factors) // 2
                first_part = factors[:half]
                second_part = factors[half:]

                # Calculate the product of the two parts
                product = 1
                for factor in first_part + second_part:
                    product *= factor

                print("{}={}*{}".format(number, "*".join(map(str, first_part)), "*".join(map(str, second_part))))

if __name__ == "__main__":
    main()
